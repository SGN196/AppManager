<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caiqian.mapper.DataDictionaryMapper">
  <select id="queryAllAppStatus" resultType="com.caiqian.Bean.DataDictionary">
    SELECT
     *
    FROM
     appinfodb.data_dictionary
    WHERE
      type_Code = 'App_STATUS'
  </select>
  <select id="queryAllAppFlatforms" resultType="com.caiqian.Bean.DataDictionary">
     SELECT
     *
    FROM
     appinfodb.data_dictionary
    WHERE
      type_Code = 'APP_FLATFORM'
  </select>
</mapper>